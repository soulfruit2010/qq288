jQuery(()=>{const $=jQuery;const forms=document.querySelectorAll('form');const exclude=['convert-to-amp'];if(document.referrer&&document.referrer.indexOf('https://ampify.io')==-1){localStorage.setItem('a-source',document.referrer);}
